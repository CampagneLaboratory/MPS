<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD" "http://plugins.intellij.net/plugin.dtd">
<idea-plugin>
  <name>JetBrains MPS Integration</name>
  <description></description>
  <version>1.0</version>
  <vendor>JetBrains</vendor>
  <idea-version since-build="3000"/>

  <application-components>
    <component>
      <interface-class>jetbrains.mps.plugin.ProjectCreator</interface-class>
      <implementation-class>jetbrains.mps.plugin.ProjectCreator</implementation-class>
    </component>
  </application-components>
  <project-components>
    <component>
      <interface-class>jetbrains.mps.plugin.MPSSupportHandler</interface-class>
      <implementation-class>jetbrains.mps.plugin.MPSSupportHandler</implementation-class>
    </component>
  </project-components>
</idea-plugin>