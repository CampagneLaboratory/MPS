<project name="buildUtilLanguages" default="build" basedir="../">
  <property name="build.dir" location="build" />
  <property name="build.tmp" location="${build.dir}/tmp/buildUtilLanguages" />
  <property name="build.layout" location="${build.dir}/artifacts/buildUtilLanguages" />
  
  <target name="assemble" />
  
  <target name="build" depends="assemble" />
  
  <target name="clean">
    <delete dir="${build.tmp}" />
    <delete dir="${build.layout}" />
  </target>
</project>