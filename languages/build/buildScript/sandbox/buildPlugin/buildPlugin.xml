<project name="buildPlugin" basedir="../">
  <property name="buildCore.artifacts" location="build/artifacts/buildCore.artifacts/buildCore.variables.properties" />
  <property file="${buildCore.artifacts}" />
  <property name="build.dir" location="build" />
  <property name="buildPlugin.tmp" location="${build.dir}/tmp/buildPlugin" />
  <property name="buildPlugin.deploy.dir" location="${build.dir}/artifacts/buildPlugin" />
  
  <target name="build" />
  
  <target name="clean">
    <delete dir="${build.dir}" />
  </target>
</project>