<project name="buildCore">
  <property name="mps_home" location="./../../../.." />
  <property name="build_langs" location="${mps_home}/languages/build" />
  <property name="build.dir" location="build" />
  <target name="java.compile.mps-core">
    <javac destdir="${build.dir}/tmp/java.out.mps-core">
      <src path="${mps_home}/core/source" />
    </javac>
  </target>
  <target name="build" depends="check, assemble"></target>
  <target name="clean">
    <delete dir="${build.dir}" />
  </target>
  <target name="compileJava" depends="java.compile.mps-core"></target>
  <target name="processResources"></target>
  <target name="classes" depends="compileJava, processResources"></target>
  <target name="assemble" depends="classes">
    <mkdir dir="${buildCore.deploy.dir}/default" />
    <mkdir dir="${buildCore.tmp}/1117643560963371560" />
    <mkdir dir="${buildCore.tmp}/1117643560963371565" />
    <mkdir dir="${buildCore.tmp}/1117643560963371565/bbb" />
    <zip destfile="${buildCore.tmp}/1117643560963371560/inrelease.zip">
      <fileset dir="${buildCore.tmp}/1117643560963371565" />
    </zip>
    <mkdir dir="${buildCore.tmp}/1117643560963371560/aaa" />
    <mkdir dir="${buildCore.tmp}/1117643560963371560/aaa/bbb" />
    <zip destfile="${buildCore.deploy.dir}/default/release.zip">
      <fileset dir="${buildCore.tmp}/1117643560963371560" />
    </zip>
    <mkdir dir="${buildCore.tmp}/6627265764879718146" />
    <mkdir dir="${buildCore.tmp}/6627265764879718146/abc" />
    <mkdir dir="${buildCore.tmp}/6627265764879718146/abc/edf" />
    <zip destfile="${buildCore.tmp}/6627265764879718146/abc/edf/aaa"></zip>
    <jar destfile="${buildCore.deploy.dir}/default/mps-core-${ver}.jar">
      <fileset dir="${build.dir}/tmp/java.out.mps-core" />
      <fileset dir="${buildCore.tmp}/6627265764879718146" />
    </jar>
  </target>
  <target name="test" depends="classes"></target>
  <target name="check" depends="test"></target>
</project>