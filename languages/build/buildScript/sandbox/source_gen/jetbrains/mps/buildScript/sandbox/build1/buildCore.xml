<project name="buildCore">
  <property name="build.dir" location="build" />
  <target name="java.compile.mps-core">
    <javac destdir="${build.dir}/temp/java/module1">
      <src path="null/source" />
    </javac>
  </target>
  <target name="build" depends="check, assemble"></target>
  <target name="clean">
    <delete dir="${build.dir}" />
  </target>
  <target name="compileJava" depends="java.compile.mps-core"></target>
  <target name="processResources"></target>
  <target name="classes" depends="compileJava, processResources"></target>
  <target name="assemble" depends="classes">
    <jar destfile="mps-core-${ver}.jar" />
    <mkdir dir="default" />
  </target>
  <target name="test" depends="classes"></target>
  <target name="check" depends="test"></target>
</project>