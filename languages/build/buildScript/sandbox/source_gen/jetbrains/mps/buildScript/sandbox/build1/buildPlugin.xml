<project name="buildPlugin">
  <property name="buildPlugin.tmp" location="${basedir}/build/tmp/buildPlugin.tmp" />
  <property name="buildPlugin.deploy.dir" location="${basedir}/build/artifacts/buildPlugin.artifacts" />
  <property name="build.dir" location="build" />
  
  <target name="build" />
  
  <target name="clean">
    <delete dir="${build.dir}" />
  </target>
</project>